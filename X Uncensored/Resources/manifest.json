{
	"name": "__MSG_name__",
	"short_name": "__MSG_shortName__",
	"description": "__MSG_description__",
	"version": "0.0.3",
	"manifest_version": 3,
	"default_locale": "en_US",
	"icons": {
		"16": "./images/icon_16x16.png",
		"24": "./images/icon_24x24.png",
		"32": "./images/icon_32x32.png",
		"48": "./images/icon_48x48.png",
		"64": "./images/icon_64x64.png",
		"96": "./images/icon_96x96.png",
		"114": "./images/icon_114x114.png",
		"120": "./images/icon_120x120.png",
		"128": "./images/icon_128x128.png",
		"144": "./images/icon_144x144.png",
		"152": "./images/icon_152x152.png",
		"180": "./images/icon_180x180.png",
		"192": "./images/icon_192x192.png",
		"256": "./images/icon_256x256.png",
		"384": "./images/icon_384x384.png",
		"512": "./images/icon_512x512.png",
		"1024": "./images/icon_1024x1024.png"
	},
	"permissions": [
		"scripting",
		"management"
	],
	"background": {
		"service_worker": "./service_worker.js",
		"scripts": [
			"./service_worker.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"https://x.com/*"
			],
			"js": [
				"./content_script.js"
			],
			"run_at": "document_start"
		}
	],
	"web_accessible_resources": [
		{
			"matches": [
				"https://x.com/*"
			],
			"resources": [
				"filter_responses.js"
			]
		}
	],
	"host_permissions": [
		"https://x.com/*"
	]
}
